 <div class="col-lg-4">
    <%= link_to "#" do %>
        <div class="panel panel-warning" id="<%= infrastructure[:id] %>">
            <div class="panel-heading">
                <%= infrastructure[:name] %>
            </div>
            <div class="panel-body text-left">
                <dl class="dl-horizontal">
                    <dt>Username</dt>
                    <dd><%= infrastructure[:username] %></dd>
                    <dt>Tenant</dt>
                    <dd><%= infrastructure[:tenant] %></dd>
                </dl>
            </div>
        </div>
    <% end %>
</div>
